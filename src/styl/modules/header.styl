.head
    & > .center 
        margin-top 22px !important
        margin-bottom 22px !important
        & .top-icons 
            display: flex
            justify-content: space-between
            color: #999999;
            font-size: 14px;
            font-weight: 400;
            flex: 1
            margin-left: 25px
            & > div 
                display: flex
                justify-content: center
                color: #999999;
                font-size: 14px;
                font-weight: 400;
                height 36px
                flex: 0 0 auto
                width: initial !important
                &.ico-carrinho
                    margin-left: 20px
                    & .icart
                        content url('data:image/png;base64,')
                        margin-right: 11px
                    & .carrinhoHead
                        & > a 
                            display: flex;
                            flex-wrap: wrap;
                            flex: 0 0 auto;
                            width: auto;
                            line-height: 1;
                            text-align: left;
                            justify-content: flex-start;
                            & > .minicart-qtde-itens
                                flex: 0 0 100%;
                                order: 3;
                                height: auto;
                                line-height: 1;
                                margin-top: -20px;
                                &:after 
                                    content: " Itens";
                                &:before 
                                    content: none
                            & > .minicart-txt-itens 
                                flex: 0 0 auto;
                                height: auto;
                                line-height: 1;
                                /* margin-left: 47px; */
                                font-size: 0;
                                &:before 
                                    content: 'Carrinho';
                                    font-size: 15px;
                                    text-align: left;
                &.ico-minha-conta
                    display: flex !important
                    justify-content: center
                    &:before 
                        content none
                    & .fa-user-circle
                        content url('data:image/png;base64,')
                & .fa-headphones
                    &:before 
                        content url('data:image/png;base64,')
                        color: #ff4444
                @media screen and (max-width: 660px)
                    &.headernav-central
                        display: none
                    &.ico-carrinho
                        &:before 
                            display: none
                        & .carrinhoHead 
                            position relative
                            & .minicart-qtde-itens
                                display: none
                    &.ico-busca
                        display: none
                & .texts
                    display: flex
                    flex-flow: column nowrap
                    justify-content: center
                    align-items: flex-start
                    line-height: 1
                    height 36px
                    margin-left: 15px
                    & span 
                        color: #999999;
                        font-size: 14px;
                        font-weight: 500;
                    & a 
                        color: #333333;
                        font-size: 14px;
                        font-weight: 500;
                        position relative !important
                        text-align: left
        & .div-busca-desktop
            width: 430px
        & .headerSearch  
            width: 100%
            & form 
                & input.inputSearch[type="text"] 
                    height: 40px;
                    border: 1px solid #d9d9d9;
                    background-color: #ffffff;
                & #btnBusca
                    /* Style for "Rectangle" */
                    width: 40px !important
                    height: 40px !important
                    background-color: #ff4444 !important
                    display: flex
                    justify-content: center
                    align-items: center !important
                    line-height: 1
                    & > i 
                        color: #FFFFFF !important
    & .topBar
        background-color: #000000
        height 53px
        & .center
            height 100%
            max-height: 100%
            padding: 0
            margin: 0 auto
            & .prolab
                &-topbar
                    background-color: #000000
                    height 100%
                    display: flex
                    justify-content: space-between
                    align-items: center
                    & i 
                        color: #ff4444
                        font-size: 15px
                    & .topbar-offer
                        flex 0 0 auto
                        padding: 0 25px
                        position relative
                        &:nth-child(5)
                            &:after 
                                content: none !important
                        &.socials
                            flex: 1
                            justify-content: center
                            display: flex
                            & a  
                                display: inline-block
                                margin-left: 11px
                                font-size: 15px;
                                &:hover 
                                    & i 
                                        color: #FFFFFF
                        &.white
                            flex: 0 0 244px
                            padding: 0
                            background-color: #FFFFFF
                            display: flex
                            justify-content: center
                            align-items: center
                            color: #333333;
                            font-size: 13px;
                            font-weight: 700;
                            line-height: 20px
                            height 100%
                            @media screen and (min-width: 1024px)
                                margin-left: 30px
                            &:after 
                                content none !important
                            & .fa-file-invoice-dollar
                                width: 27px
                                height 27px
                                display: inline-block
                                font-family: initial !important
                                margin-right: 12px
                                &:before
                                    content ''
                                    background-image: url('data:image/svg+xml;base64,')
                                    width: 27px
                                    height 27px
                                    background-size: 100%
                                    background-position: center 
                                    background-repeat: no-repeat
                                    display: block
                            & span 
                                display: flex
                                flex-flow: column nowrap
                                justify-content: center
                                & h2 
                                    text-transform: uppercase
                                    font-weight: 700;
                                    font-size: 13px;
                            & a 
                                color: #ff4444;
                                font-weight: 700;
                                text-decoration: underline;
                            & i 
                                font-size: 30px
                        & a 
                            color: #ffffff;
                            font-size: 14px;
                            font-weight: 400;
                            text-decoration: none
                            &:hover
                                text-decoration: underline
                        &:not(:last-child)
                            &:after 
                                content: ''
                                width: 1px
                                height 15px
                                background-color: #333333
                                position: absolute
                                right -0.5px
                                top: 50%
                                margin-top: -7.5px

.header 
    position: fixed
    top: 0
    left: 0
    width: 100%
    z-index: 99
    display: flex
    flex-wrap: wrap
    &__navigation
        flex: 0 1 425px
        @media(max-width: 991px)
            display: none
    .topbar
        &__right
            width: 300px;
            heght: 100%
            ul
                display flex
                justify-content: space-between
                li
                    background-color: #56a5c4
                    height 50px
                    flex: 1 1 33%
                    display: flex 
                    justify-content: center
                    align-items: center
                    margin 0
                    &:first-child
                        border-right: 1px dashed #89c0d6
                        border-left: 1px dashed #89c0d6
                    &:last-child
                        border-left: 1px dashed #89c0d6
                        border-right: 1px dashed #89c0d6
            .topbar-user
                position: relative
            .topbar-whishlist
                background-image: url('data:image/png;base64,')
                background-repeat: no-repeat
                background-size: auto
                background-position: center
                a
                    display: block
                    width: 100%
                    height 100%

        &__info
            display: flex 
            justify-content: space-between
            align-items: center
            flex: 0 1 360px
            span 
                color: #ffffff
                font-size: 13px
                display flex
                align-items: center
        &__whats
            &:before
                content: ''
                margin-right 5px
                width: 12px
                height: 12px
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-size: auto 
                background-position center
            &:after
                content: '|'
                display: inline-block
                color: #ffffff
                padding-left 21px
        &__email
            &:before
                content: ''
                margin-right 5px
                width: 15px
                height: 13px
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-size: auto 
                background-position center
    &__topbar
        background-color: $cor01
        width: 100%
        height 50px
        transition: all .3s
        
        @media(max-width : 991px)
            order: 1
            display none
        &-container
            max-width 1200px
            width: 100%
            height 50px
            margin: 0 auto
            display: flex
            align-items: center
            justify-content: space-between
            min-height 35px

            ul
                display flex
                align-items: center

                li
                    margin 0 3px

                span
                    font-size: 13px
                    color: #fffbfc
                    margin-right: 10px
                    text-transform: uppercase
            ul.telefone__mid 
                li 
                    color: #fff
                    font-size: 13px
                    font-weight: 600
                    display: flex
                    align-items: center

                    img
                        margin-right 5px

                    &:last-child
                        &:before
                            content: '|'
                            margin 0 10px 0 5px
                            font-weight 300

            .links__topbar
                li
                    &:last-child
                        &:before
                            content: '|'
                            margin 0 10px 0 5px
                            font-weight 300
                            color: #fff
                    a
                        font-size 13px
                        color: #fff
            @media(max-width: 991px)
                height initial
                padding-bottom 20px
                border-bottom: 1px solid #cecece
                .links__topbar
                    li
                        a
                            font-size: 16px
                            color: #020202 !important

        &-list
            padding-left: 220px
            display: flex
            align-items: center
            justify-content: space-between
            @media(max-width : 1249px)
                max-width: 55%
                padding: 0
            @media(max-width : 991px)
                max-width: 100%
                padding: 0 30px
        .slick-prev
            left 0
            width: 30px
            height 30px
        .slick-next
            right 0
            width: 30px
            height 30px
        &-item
            padding-right: 35px
            font-size: 12px
            line-height: 15px
            font-weight: 800
            color: #FFFFFF
            display: flex!important
            align-items: center
            @media(max-width : 1300px)
                padding-right: 15px
            @media(max-width : 1249px)
                justify-content: center
            @media(max-width : 991px)
                padding: 0
            &--two
                @media(max-width : 991px)
                    font-size: 0px
                    &:after
                        content: '25 anos no mercado'
                        font-size: 12px

        &-phones
            display: flex
            align-items: center
            justify-content: space-between
            @media(max-width : 991px)
                display: none
        &-phone
            font-size: 12px
            color: #FFFFFF
            line-height: 15px
            display: flex
            align-items: center

            @media(max-width 991px)
                color: #333
                font-weight: 600
            &--one
                padding-right: 20px
    &__mainbar
        background: #fff;
        position: relative
        z-index: 1
        width: 100%
        border-bottom: 1px solid #cecece
        z-index: 999
        
 
        &-container
            max-width 1200px
            width: 100%
            margin: 0 auto
            
        &-wrapper
            display: flex
            align-items: center
            justify-content: space-between
            min-height: 100px
            padding: 15px 0
            @media(max-width : 991px)
                flex-wrap: wrap

                .menu   
                    display none!important
        &-mobile
            display: none
            @media(max-width : 991px)
                display: block
                width: 27px
                height 22px


        &-logo
            position: relative
            display: flex
            align-items: center

            .headerLogo
                max-width: 130px
                width: 130px
                img 
                    max-width: 130px

            @media(max-width : 991px)
                width 33%
                justify-content center

                a
                    width 100%

        &-serach
            max-width: 360px
            width: 100%
            position relative
            @media(max-width : 991px)
                max-width: 100%
                order 1
                padding-bottom: 11px
                padding-top: 31px
                transition: all .3s
            .headerSearch
                width: 100%
                float: none
                margin: 0
                opacity: 1
                visibility: visible

            input.inputSearch 
                font-size: 13px
                color: #666666 
                border: 1px solid #d9d9d9 !important;
                &::placeholder
                    font-size: 13px
                    color: #666666

            button#btnBusca.btSearch
                position absolute
                background-image: url('data:image/svg+xml;base64,')
                background-repeat: no-repeat
                background-position: center
                background-size: 15px
                border-radius 50px
                height 39px
                width: 40px
                &:hover
                    color: #ffffff
                .icon.isearch
                    display: none
                    
        &-account, &-order, &-contact
            border-right: 1px solid #D8D8D8
            padding: 12px 20px
            @media(max-width : 991px)
                display: none
            a
                color: #373535
                font-size: 12px
                text-transform: uppercase
                font-weight: 600
                white-space: nowrap
                border-bottom: 1px solid transparent
                &:hover
                    border-bottom: 1px solid #fff115

        &-right
            display: flex
            align-items: center
            justify-content: space-between
            max-width: 360px
            width 100%
            position relative

            @media(max-width : 991px)
                width 100%
                max-width initial !important

        &-links
            display: flex 
            align-items: center
            justify-content: center;
            width: 100%;
            height: 100%;
            img
                max-width: 40px;
            span
                font-size 13px
                font-weight: 600
                color: #333
                display: block

                a
                    display block
                    color: #333
                    font-weight: 800
                    &:after 
                        content: '';
                        display: inline-block;
                        width: 10px;
                        height: 10px;
                        margin-left: 5px;
                        background-image: url('data:image/svg+xml;base64,');
                        background-repeat: no-repeat;
                        background-position: center;
                        background-size: 10px;
            &.atendimento
                position relative
                .atendimento__sub
                    display: none;
                    position: absolute
                    top: 50px
                    left: 50%
                    transform: translateX(-50%)
                    padding: 20px 15px
                    background-color: #ffffff
                    border: 1px solid #d2d2d2
                    transition: 1s
                    h3
                        font-size: 15px
                        text-transform: uppercase
                        font-weight: 800
                        margin-bottom: 10px
                        color: $cor02
                        &:before
                            content: ''
                            width: 100%
                            height: 15px 
                            position absolute
                            top: 0
                            left: 0
                            background-color: #fff
                    &:before
                        content: ''
                        width: 15px 
                        height: 15px
                        transform: rotate(45deg)
                        position: absolute
                        top: -8px
                        left: 47%
                        background-color: #d2d2d2
                    &:after
                        content: ''
                        width: 100% 
                        height: 15px
                        position: absolute
                        top: -15px
                        left: 0
                        background-color: transparent
                &:hover
                    .atendimento__sub
                        display: block
            &.user-bemvindo
                .login__sub
                    display: none;
                    position: absolute
                    top: 50px
                    left: 50%
                    transform: translateX(-50%)
                    padding: 15px
                    background-color: #ffffff
                    border: 1px solid #d2d2d2
                    transition: 1s
                    z-index: 9999
                    h3
                        font-size: 15px
                        text-transform: uppercase
                        font-weight: 800
                        margin-bottom: 10px
                        color: $cor02
                        &:before
                            content: ''
                            width: 100%
                            height: 15px 
                            position absolute
                            top: 0
                            left: 0
                            background-color: #fff
                    a
                        text-transform uppercase
                        padding 10px
                        color: #ffffff
                        font-weight: 800
                        text-align center
                        min-width: 160px;
                        margin: 10px 0;
                        &:first-child
                            background-color: $cor01
                        &:last-child
                            background-color: $cor02
                        &:after
                            display: none
                    &:before
                        content: ''
                        width: 15px 
                        height: 15px
                        transform: rotate(45deg)
                        position: absolute
                        top: -8px
                        left: 47%
                        background-color: #d2d2d2
                    &:after
                        content: ''
                        width: 100% 
                        height: 15px
                        position: absolute
                        top: 0
                        left: 0
                        background-color: #fff
                &:hover
                    .login__sub
                        display: block
        &-cart
            display: flex 
            span
                font-size: 13px;
                font-weight: 600;
                color: #fff;
                margin-left: 5px;
            span.minicart-qtde-itens
                &:before
                &:after 
                    display: none !important
            @media(max-width : 991px)
                padding: 0
            .carrinho.carrinhoHead
                a
                    float none
                    display: flex!important
                    align-items: center
                .icon.icart
                    display: none
                .minicart-txt-itens
                    display: none
                .minicart-qtde-itens
                    color: #fff !important
                    font-size: 15px
                    color: #333;
                    float: none
                    position: absolute
                    left: 25px
                    bottom: -8px

    @media(max-width: 1290px)
        &__topbar-container
        &__mainbar-container
            max-width: 991px
        &__mainbar 
            &-cart
                span 
                    display none
                span.minicart-qtde-itens
                    display block
                    background-color transparent 
                    left: 10px !important
                    &:before
                    &:after
                        display none !important
                .carrinho.carrinhoHead
                    position relative
                    a
                        margin-right: 10px;
                span.minicart-qtde-itens
                    left: 10px;
            &-right
                max-width: 315px;
    @media(max-width: 991px)
        &__mainbar
            &-container
                padding 0 15px
            &-links
                display none
            &-logo
                width: 60%
                .headerLogo
                    max-width: 100% !important 
                    img 
                        max-width: 100% !important
            &-mobile
                height: 30px
            &-serach
                padding-top 10px
            &-cart
                &.mobile
                    margin-right 10px
                    .carrinho.carrinhoHead a
                        justify-content: center;
                        span.minicart-qtde-itens
                            color: #333 !important;
                            float: none;
                            position: absolute;
                            background-color: #fff;
                            bottom: -0.5rem;
                            right: -10px;
                            left: initial !important;
                            border: 2px solid #007e97;
                            padding: 0;
                            display: flex;
                            justify-content: center;
                            align-items: center;
        

    