html body .footer
    &__rodape
        & .copyright-footer
            border: 1px solid #e6e6e6;
            background-color: #f3f3f3;
            color: #666666;
            font-size: 14px;
            font-weight: 400;
            line-height: 24px;
        &-columns
            display: flex
            justify-content: space-between
            margin-bottom: 45px
            @media screen and (max-width: 992px)
                flex-wrap: wrap
            & .pagamentos
                flex: 0 0 calc(40% - 50px)
                @media screen and (max-width: 992px)
                    flex: 0 0 100%
                & > section 
                    display: flex 
                    flex-flow: column nowrap
                    & > ul 
                        margin-bottom: 10px
                        @media screen and (min-width: 1024px)
                            padding-left: 0
                            width: 100%
                            height: auto
                            border: 1px solid #d9d9d9;
                            display: flex
                            flex-flow: column
                            padding: 14px
                        & > li.li-formas-pgto
                            display: flex
                            justify-content flex-start
                            flex-wrap: wrap
                            align-items: center
                            @media screen and (max-width: 992px)
                                display: flex !important
                            & > span 
                                line-height: 1
                                vertical-align: middle
            & .paginas
                flex 0 0 calc(60% - 50px)
                display: flex
                justify-content: space-between
                @media screen and (max-width: 992px)
                    flex: 0 0 100%
                    flex-wrap: wrap
                & > section 
                    flex 0 0 auto
                    width: auto
                    @media screen and (max-width: 992px)
                        flex: 0 0 100%
                        width: 100%
                    & > ul 
                        color: #333333;
                        font-size: 14px;
                        font-weight: 400;
                        line-height: 30px;
                        width: auto
                        @media screen and (max-width: 992px)
                            width: 100%
                            border-bottom: 1px solid #e1e1e1 ;
                        & > strong 
                            font-weight: 700
                        & > li 
                            & .contato-fone,
                            & .contato-mail
                                color: #333333;
                                font-size: 14px;
                                font-weight: 400;
                                line-height: 1;
                                min-height: initial
                                margin 7.5px 0
                                float none
                            &.street 
                                & span 
                                    display: inline-block
                                    vertical-align: top
                                    line-height: 16px
                            @media screen and (max-width: 992px)
                                padding 5px 0
                            &:first-child
                                &, & > a 
                                    font-size: 14px;
                                    line-height: 30px;
                                    color: #ff4444;
                                    font-weight: 700;
                                    text-transform: initial
                            & > a 
                                color: #333333;
                                font-size: 14px;
                                font-weight: 400;
                                line-height: 30px;

    &__conteudo
        display: flex
        flex-flow: column nowrap
        align-items: center
        width: 100%
        &-container
            padding: 40px
            display: flex
            flex-flow: column nowrap
            align-items: center
            width: 100%
            max-width: 1300px
            @media screen and (max-width: 480px)
                padding: 20px 10px
        &-content
            display: flex
            justify-content: flex-start
            color: #ffffff;
            font-size: 14px;
            font-weight: 400;
            line-height: 24px;
            text-align: left 
            @media screen and (max-width: 480px)
                flex-wrap: wrap
            & .texts
                flex: 1
                margin-left: 30px
                @media screen and (max-width: 480px)
                    margin-left: 0
                    text-align: justify
                    & tr 
                        display: flex;
                        flex-wrap: wrap;
                & td 
                    padding: 0 10px
                    vertical-align: top
                    @media screen and (max-width: 480px)
                        padding 0
        & h3
            color: #ff4444;
            font-size: 25px;
            font-weight: 400;
            text-transform: uppercase;
            padding-bottom 17px
            border-bottom: 1px solid rgba(255,255,255,0.12)
            width: 100%
            display: block
            text-align: center
            margin-bottom: 30px
            & strong 
                font-weight: 700;
    &__news
        display: flex
        background-color: #000000
        margin: 0 
        padding: 0
        height 80px
        margin: 0
        margin-top: 30px
        @media screen and (max-width: 480px)
            flex-wrap: wrap
            height auto
        &-column
            flex: 0 0 50%
            display: flex
            height 100%
            justify-content: center
            align-items: center
            &.input
                padding 0 25px
                @media screen and (max-width: 480px)
                    flex 0 0 100%
                    width: 100%
                    padding: 0
                    display: flex
                    flex-wrap: wrap
                    height auto
            &:first-child
                @media screen and (max-width: 480px)
                    display: none !important
                & a 
                    display: flex
                    justify-content: center
                    align-items: center
                    height 100%
                    overflow hidden
                & img 
                    height 100%
                    width: auto
                    transition: all 200ms ease-in
                    &:hover
                        transform: perspective(1000px) translateZ(50px);
                    @media screen and (max-width: 480px)
                        max-width: initial
                @media screen and (max-width: 480px)
                    width: 100%
                    flex: 0 0 100%
                    overflow hidden
            &-title
                flex: 0 0 180px
                color: #ffffff;
                font-size: 15px;
                font-weight: 700;
                text-transform: uppercase;
                text-align: left 
                @media screen and (max-width: 480px)
                    flex: 0 0 100%
                    text-align: center
                    margin-top: 15px
            &-input
                flex: 1
                & .footerNewsForm
                    width: 100% !important
                    height 100%
                    margin-left: 20px
                    justify-content: flex-end
                    @media screen and (max-width: 480px)
                        display: flex
                        flex-wrap: wrap
                        justify-content: center
                        margin-left: 0
                        padding:15px 10px
                    & .btNews
                        width: 50px
                        height 30px
                        background-color: #ff4444;
                        color: #ffffff;
                        font-size: 14px;
                        font-weight: 700;
                        text-transform: uppercase;
                        margin: 0
                        margin-left: 4px
                        @media screen and (max-width: 480px)
                            width: 100%
                            margin-left: 0
                    & .newsletter-box
                        display: flex
                        flex-flow: row nowrap
                        justify-content: space-between
                        @media screen and (max-width: 480px)
                            display: flex
                            flex-wrap: wrap
                            justify-content: center
                            margin-left: 0
                        & input
                            flex: 0 0 calc(50% - 4.5px)
                            height 30px
                            &, &::placeholder
                                color: #333333;
                                font-family: "Open Sans";
                                font-size: 14px;
                                font-weight: 400;
                            @media screen and (max-width: 480px)
                                flex: 0 0 100%
                        &:nth-child(2)
                            display: none
